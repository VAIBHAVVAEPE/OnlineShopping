
@{
    ViewData["Title"] = "SignIn";
}

<h1>Sign In</h1>

<div class="container">
     @{Html.BeginForm();}
     <div class="row">
        <div class="col-lg-3">
            @Html.Label("Email Id")
        </div>
        <div class="col-lg-3">
            @Html.TextBox("EmailId")
        </div>
    </div>
    <div class="row">
        <div class="col-lg-3">
            @Html.Label("Password")
        </div>
        <div class="col-lg-3">
            @Html.TextBox("Password")
        </div>
    </div>
    <div class="row">
        <div class="col-lg-3">
            
        </div>
        <div class="col-lg-3">
            <input type="Submit" value="Login" class="btn btn-success"/>
        </div>
    </div>
     @{Html.EndForm();}
</div>